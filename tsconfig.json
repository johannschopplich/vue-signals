{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM", "ScriptHost"],
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Node",
    "paths": {
      "vue-signals": ["./src/index.ts"]
    },
    "types": ["vite/client"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
