{
  "name": "vue-signals",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.27.0",
  "description": "Solid.js-style signals for Vue.js",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/vue-signals#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/vue-signals.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/vue-signals/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp --commit --push --tag"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.34.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.2"
  }
}
